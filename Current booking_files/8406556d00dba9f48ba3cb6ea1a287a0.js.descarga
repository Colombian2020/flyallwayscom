      }

      var totalFeeAmount = fl.taxes.fee_total + ssrsVatAmount;
      fl.taxes.fee_total_formatted = WT.Utiles.formatWithCurrency(totalFeeAmount.toFixed(2), format);
    }

    fl.added_type = fl_type;

    if (WT.Modify.order) {
