        return parseFloat(("" + price).replace(/,/g, '.'));
      }
    };

    if (fl) {
      var f_id = fl.trip_id,
        taxes = 0,
        total_fare = 0,
        base_fare = 0,
        total_discount = 0,
        total_fare_discounted = 0,
        base_fare_discounted = 0,
