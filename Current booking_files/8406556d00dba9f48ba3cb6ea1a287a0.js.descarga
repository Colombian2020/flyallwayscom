    }

    if (WT.ReservationData.FlashSaleData) {
      selectFlashSaleFare();
    } else {
      switch (WT.Options.preselectFare) {
        case 'preselect_lowest_fare':
          if (result && result.outbound) {
            selectLowestFare('outbound');
