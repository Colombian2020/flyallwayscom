      if (!isReservedData) return false;

      var reservedFromProduct = (step === 'seats')
        ? WT.ReservationData[step]
        : WT.ReservationData[step]['from_' + step],
        reservedToProduct = (step === 'seats')
          ? WT.ReservationData[step]
          : WT.ReservationData[step]['to_' + step],
