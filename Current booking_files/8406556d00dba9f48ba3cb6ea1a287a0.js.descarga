    $j.ajax({
      url: baseUrl + 'sales/order/cancelSegment?order_id=' + orderId,
      type: 'POST',
      dataType: 'json',
      data: data,
      success: function (result) {
        if (!result.status) {
          return location.reload();
        }

        if (!isRefund) return location.reload();

        WT.Cancel.refundRequestBySegment();
